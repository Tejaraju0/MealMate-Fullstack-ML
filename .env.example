# MealMate Environment Variables Template
# Copy this file to .env and fill in your actual values

# Backend Configuration
# Server Port
PORT=5000

# MongoDB Connection
MONGO_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/mealmate?retryWrites=true&w=majority

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-min-32-characters-long

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=123456789012345
CLOUDINARY_API_SECRET=your-api-secret

# Service URLs
REACT_APP_SERVER_URL=http://localhost:5000
FRONTEND_URL=http://localhost:3000
ML_SERVICE_URL=http://localhost:5001

# Google Maps API Key
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Stripe Test Keys
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxx
STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxx

# Encryption Key
ENCRYPTION_KEY=your-encryption-key-32-characters-long

# SMTP Email Configuration
SMTP_EMAIL=your-email@gmail.com
SMTP_PASS=your-app-specific-password

# Frontend Configuration

# React Stripe Keys
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxx
REACT_APP_STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxx
